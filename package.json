{
  "name": "@tvrcgo/create",
  "version": "0.1.3",
  "description": "Create project from boilerplates",
  "bin": {
    "create": "bin/cli.js"
  },
  "files": [
    "bin",
    "boilerplates",
    "lib"
  ],
  "scripts": {
    "start": "node bin/cli.js"
  },
  "config": {
    "registry": "https://npm.pkg.github.com",
    "boilerplates": "@tvrcgo/create",
    "types": [
      {
        "name": "Egg application",
        "value": "egg-app"
      },
      {
        "name": "Egg plugin",
        "value": "egg-plugin"
      },
      {
        "name": "Umi 3",
        "value": "umi"
      }
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tvrcgo/create.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.0",
    "compressing": "^1.5.1",
    "globby": "^11.0.1",
    "inquirer": "^7.3.3",
    "istextorbinary": "^5.11.0",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "ora": "^5.1.0",
    "rimraf": "^3.0.2",
    "urllib": "^2.36.1"
  }
}
